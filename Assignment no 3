package com.Training;

public class Que3 {

	public static void main(String[] args) {
	int emp1=1100;//1st Employee
	int emp2=500;//2nd Employee
	int emp3;//3rd Employee
	// avg= emp1+emp2+emp3/3 
	int avg=1000*3;
	int sum=emp1+emp2;
    emp3=avg-sum;
    System.out.println("Thrid employee earn "+emp3);
	
	
	
	}

}
